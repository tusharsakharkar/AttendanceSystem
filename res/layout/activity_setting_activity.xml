<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Setting_activity" >

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="0dp"
    android:background="@color/lighter_gray"
    android:orientation="vertical" >
     <LinearLayout
         android:layout_width="match_parent"
         android:layout_height="60dp"
         android:background="@drawable/upper" >
              </LinearLayout>
         
         <LinearLayout
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:layout_marginTop="02dp"
             android:background="@color/accessibility_focus_highlight"
             android:orientation="vertical" >

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/red"
    android:orientation="vertical" >
    
     <TextView
         android:id="@+id/textView1"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_gravity="left"
         android:layout_marginBottom="10dp"
         android:layout_marginLeft="20dp"
         android:layout_marginTop="12dp"
         android:text="Settings"             android:textStyle="bold"
         
         android:textAppearance="?android:attr/textAppearanceLarge"
         android:textColor="@color/white1" />

     </LinearLayout>
 </LinearLayout>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="350dp"
    android:orientation="vertical" >
<LinearLayout
                 android:layout_width="match_parent"
                 android:layout_height="43dp"
                 android:layout_marginLeft="05dp"
                 android:layout_marginRight="05dp"
                 android:layout_marginTop="20dp"
                 android:background="@color/white1"
                 android:orientation="horizontal" >
                 
                 <TextView
                     android:id="@+id/textView1"
                     android:layout_width="30dp"
                     android:layout_height="30dp"
                     android:layout_marginTop="0dp"
                     android:layout_marginLeft="10dp"
                     android:layout_marginBottom="02dp"
                     android:background="@drawable/jana"
                     android:text="" />

                 <Button
                     android:id="@+id/setimp"
                     android:layout_width="match_parent"
                     android:layout_height="30dp"
                                         android:gravity="left|center_vertical"
                    android:paddingLeft="20dp"
                     
                                          android:layout_marginTop="07dp"
                     android:layout_marginRight="10dp"
                     android:background="@drawable/samplebtn"
                     android:text="Import Data" />
                 
             </LinearLayout>
 <LinearLayout
                 android:layout_width="match_parent"
                 android:layout_height="43dp"
                 android:layout_marginLeft="05dp"
                 android:layout_marginRight="05dp"
                 android:layout_marginTop="7dp"
                 android:background="@color/white1"
                 android:orientation="horizontal" >
                 
                 <TextView
                     android:id="@+id/textView1"
                     android:layout_width="30dp"
                     android:layout_height="30dp"
                     android:layout_marginTop="0dp"
                     android:layout_marginLeft="10dp"
                     android:layout_marginBottom="02dp"
                     android:background="@drawable/xpo"
                     android:text="" />

                 <Button
                     android:id="@+id/set11"
                     android:layout_width="match_parent"
                     android:layout_height="30dp"
                                         android:gravity="left|center_vertical"
                    android:paddingLeft="20dp"
                     
                                          android:layout_marginTop="07dp"
                     android:layout_marginRight="10dp"
                     android:background="@drawable/samplebtn"
                     android:text="Export Data" />
                 
             </LinearLayout>  
             
 <LinearLayout
                 android:layout_width="match_parent"
                 android:layout_height="43dp"
                 android:layout_marginLeft="05dp"
                 android:layout_marginRight="05dp"
                 android:layout_marginTop="30dp"
                 android:background="@color/white1"
                 android:orientation="horizontal" >
                 
                 <TextView
                     android:id="@+id/textView1"
                     android:layout_width="30dp"
                     android:layout_height="30dp"
                     android:layout_marginTop="0dp"
                     android:layout_marginLeft="10dp"
                     android:layout_marginBottom="02dp"
                     android:background="@drawable/help"
                     android:text="" />

                 <Button
                     android:id="@+id/about"
                     android:layout_width="match_parent"
                     android:layout_height="30dp"
                                         android:gravity="left|center_vertical"
                    android:paddingLeft="20dp"
                     
                                          android:layout_marginTop="07dp"
                     android:layout_marginRight="10dp"
                     android:background="@drawable/samplebtn"
                     android:text="About app" />
                 
             </LinearLayout>
             <LinearLayout
                 android:layout_width="match_parent"
                 android:layout_height="43dp"
                 android:layout_marginLeft="05dp"
                 android:layout_marginRight="05dp"
                 android:layout_marginTop="20dp"
                 android:background="@color/white1"
                 android:orientation="horizontal" >
                 
                 <TextView
                     android:id="@+id/textView1"
                     android:layout_width="30dp"
                     android:layout_height="30dp"
                     android:layout_marginTop="0dp"
                     android:layout_marginLeft="10dp"
                     android:layout_marginBottom="02dp"
                     android:background="@drawable/developer"
                     android:text="" />

                 <Button
                     android:id="@+id/about111"
                     android:layout_width="match_parent"
                     android:layout_height="30dp"
                                         android:gravity="left|center_vertical"
                    android:paddingLeft="20dp"
                     
                                          android:layout_marginTop="07dp"
                     android:layout_marginRight="10dp"
                     android:background="@drawable/samplebtn"
                     android:text="About developer" />
                 
             </LinearLayout>
</LinearLayout>
 
              </LinearLayout>
</RelativeLayout>
